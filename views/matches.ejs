<html>
<head>
  <% include partials/head %>
</head>

<body class="text-center">
  <div class="cover-container d-flex h-100 p-3 mx-auto flex-column">
    <header class="masthead mb-auto">
      <% include partials/header %>
    </header>



    <main role="main" class="inner cover">
      <h1 class="cover-heading"><%= title %></h1>

      <a href="#" data-id="<%=draw._id%>" class="rollDraw btn btn-lg btn-primary">Roll</a>

      <div class="list-group">
        <% matches.forEach(function(match){ %>
          <div class="list-group-item list-group-item-action" type="password" ><%= match.fromName %> -> <%= match.toName %>
          </div>


          <div class="btn-group" role="group">
            <!--<a href="/match/<%=match._id%>" class="btn btn btn-default">Edit</a>
            <a href="" class="deleteMatch btn btn btn-danger" data-id="<%=match._id%>">Delete</a> -->
          </div>
          <% }) %>
        </div>

        <%if (modal) { %>
          <div class="alert alert-success" role="alert">
            <%=modalText%>
          </div>
          <% } %>
        </main>


        <footer class="mastfoot mt-auto">
          <div class="inner">
            <% include partials/footer %>
          </div>
        </footer>
      </div>
    </body>
    </html>
